{
  "name": "@diplodoc/latex-extension",
  "version": "1.1.0",
  "description": "Extension for Diplodoc platform which adds support for Latex syntax using Katex",
  "main": "plugin/index.js",
  "types": "plugin/index.d.ts",
  "exports": {
    ".": "./plugin/index.js",
    "./plugin": "./plugin/index.js",
    "./runtime": "./runtime/index.js",
    "./runtime/styles": "./runtime/index.css",
    "./react": "./react/index.js",
    "./hooks": "./react/index.js"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:js": "./esbuild/build.js",
    "build:declarations": "tsc --emitDeclarationOnly --outDir .",
    "prepublishOnly": "npm run build",
    "test": "exit 0",
    "lint": "lint update && lint",
    "lint:fix": "lint update && lint fix",
    "pre-commit": "lint update && lint-staged",
    "prepare": "husky"
  },
  "author": "",
  "license": "MIT",
  "files": [
    "plugin",
    "runtime",
    "react"
  ],
  "devDependencies": {
    "@diplodoc/lint": "^1.1.3",
    "@diplodoc/transform": "^4.7.1",
    "@diplodoc/tsconfig": "^1.0.2",
    "@types/katex": "^0.16.7",
    "@types/markdown-it": "^13.0.7",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.45",
    "esbuild": "^0.19.9",
    "esbuild-inline-sass": "^0.4.3",
    "katex": "^0.16.9",
    "markdown-it": "^13.0.2",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "katex": "^0.16.9",
    "markdown-it": "^13.0.0",
    "react": "^18.2.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
}
